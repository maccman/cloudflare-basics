{
  "name": "cloudflare-basics",
  "type": "module",
  "version": "0.0.8",
  "packageManager": "pnpm@8.5.1",
  "description": "",
  "author": "Alex MaccCaw <alex@alexmaccaw.com>",
  "license": "MIT",
  "homepage": "https://github.com/maccman/cloudflare-basics#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/maccman/cloudflare-basics.git"
  },
  "bugs": "https://github.com/maccman/cloudflare-basics/issues",
  "keywords": [],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./middleware/zod-validation": {
      "types": "./dist/middleware/zod-validation.d.ts",
      "import": "./dist/middleware/zod-validation.js",
      "default": "./dist/middleware/zod-validation.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ],
      "middleware/zod-validation": [
        "dist/middleware/zod-validation.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.3",
    "@cloudflare/workers-types": "^4.20230511.0",
    "@ocavue/eslint-config": "^0.4.7",
    "@types/node": "^18.16.9",
    "eslint": "^8.40.0",
    "esno": "^0.16.3",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "urlpattern-polyfill": "^8.0.2",
    "vite": "^4.3.5",
    "vitest": "^0.31.0",
    "zod": "^3.22.1"
  },
  "renovate": {
    "extends": [
      "github>ocavue/config-renovate"
    ]
  },
  "peerDependencies": {
    "zod": "^3.22.1"
  }
}
